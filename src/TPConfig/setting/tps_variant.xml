  <Var Name="viom1A2atp" Type="int"  IO="Input" Value="5889" Description="viom1a to atp message"/>
  <Var Name="viom1B2atp" Type="int"  IO="Input" Value="5890" Description="viom1b to atp message"/>
  <Var Name="viom2A2atp" Type="int"  IO="Input" Value="5891" Description="viom2a to atp message"/>
  <Var Name="viom2B2atp" Type="int"  IO="Input" Value="5892" Description="viom2b to atp message"/>
  <Var Name="ccnv2atp" Type="int"  IO="Input" Value="5121" Description="ccnv to atp message"/>
  <Var Name="rs2atp" Type="int"  IO="Input" Value="300" Description="rs to atp control message"/>
  <Var Name="resetAtpCommand" Type="int"  IO="Input" Value="260" Description="reset atp control message"/>
  <Var Name="StartCommand" Type="int"  IO="Input" Value="261" Description="reset atp control message"/>
  <Var Name="cileft2atp" Type="int"  IO="Input" Value="15361" Description="ci left to atp message"/>
  <Var Name="ciright2atp" Type="int"  IO="Input" Value="15362" Description="ci right to atp message"/>
  <Var Name="Lc2AtpMesID" Type="int" IO="Input" Value="10241" Description="Lc2AtpMesID"/>
