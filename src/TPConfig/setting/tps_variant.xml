  <Var Name="loadDevice" Type="string"  IO="Input" Value="rs" Description="load device"/>
  <Var Name="defDeviceId" Type="int"  IO="Input" Value="1" Description="default device id"/>
  <Var Name="rs2cbkMsgId" Type="int"  IO="Input" Value="257" Description="rs to cbk msg id"/>
