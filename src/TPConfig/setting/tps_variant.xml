  <Var Name="cmdIniloophour" Type="int"  IO="Input" Value="94" Description="simulator cmd initialize loophour"/>
  <Var Name="rmsHead" Type="int"  IO="Input" Value="1001" Description="msgid of rms"/>
  <Var Name="sigHead" Type="int"  IO="Input" Value="1002" Description="msgid of sig"/>
  <Var Name="tps2sdtsMesId" Type="int"  IO="Input" Value="1003" Description="msgid of sig"/>
