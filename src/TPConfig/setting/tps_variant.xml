  <Var Name="ci2ccVariantRequest" Type="int"  IO="Input" Value="15364" Description="ci to cc variant request message"/>
  <Var Name="datp2atp" Type="int"  IO="Input" Value="1793" Description="ccnv to atp message"/>
  <Var Name="cmdCog" Type="int"  IO="Input" Value="90" Description="simulator cmd cogNumber"/>
