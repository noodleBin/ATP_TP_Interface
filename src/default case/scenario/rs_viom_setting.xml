    <VIOM_OUT>
      <Item Index="3"  Name="OUT_DE_A1"  VIOM = "0" Description="DE_A1"/>
      <Item Index="3"  Name="OUT_DE_A2"  VIOM = "1" Description="DE_A2"/>
      <Item Index="4"  Name="OUT_DE_B1"  VIOM = "0" Description="DE_B1"/>
      <Item Index="4"  Name="OUT_DE_B2"  VIOM = "1" Description="DE_B2"/>
